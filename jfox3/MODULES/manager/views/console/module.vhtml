<html>
<head>
    <link href="console/styles.css" rel="stylesheet" type="text/css">
    <title>JFox Management Console</title>
</head>
<body bgcolor="#FFFFFF">
<table cellspacing="4" width="100%" border="0">
    <tr>
        <td><a href="#"><img border="0" alt="JFox 3" align="left" src="images/jfox.jpg"></a>
            <a href="http://tomcat.apache.org/"> <img border="0" alt="The Tomcat Servlet/JSP Container" align="right" src="images/tomcat.gif"> </a>
        </td>
    </tr>
</table>
<hr size="1" noshade="noshade">
<table cellspacing="0" cellpadding="3" width="100%" border="1">
    <tr>
        <td colspan="5" class="title"><big>Menu</big></td>
    </tr>
    <tr>
        <td class="header-center">
            <small><a href="console.sysinfo.do">System Infomation</a></small>
        </td>
        <td class="header-center">
            <small><a href="console.jndi.do">JNDI View</a></small>
        </td>
        <td class="header-center">
            <small><a href="console.container.do">EJB Container</a></small>
        </td>
        <td class="header-center">
            <small><a href="console.jpa.do">Persistence Unit</a></small>
        </td>
        <td class="header-center">
            <small><a href="console.modules.do">Modules</a></small>
        </td>
    </tr>
</table>
<br>
<table cellspacing="4" width="100%" border="0">
    <tr>
        <td class="page-title" bordercolor="#000000" align="left" nowrap><font size="+2">Modules Manager</font></td>
    </tr>
</table>
#foreach($module in $modules)
<br>
<table border="1" cellspacing="0" cellpadding="3">
    <tr>
        <td colspan="5" class="title" title="$module.getDescription()">Module($module.getName())</td>
    </tr>
    <tr>
        <td colspan="5" class="row-left" bgcolor="#FFFFFF">
            <small>
                <table border="0" cellspacing="0">
                    <tr>
                        <td class="header-center" bgcolor="#FFFFFF">Descprtion</td>
                        <td class="row-left"> $module.getDescription()</td>
                    </tr>
                    <tr>
                        <td class="header-center">Priority</td>
                        <td class="row-left">$module.getPriority()</td>
                    </tr>
                    <tr>
                        <td class="header-center">Module Dir</td>
                        <td class="row-left">$!module.getModuleDir()</td>
                    </tr>
                    <tr>
                        <td class="header-center">Reference Modules</td>
                        <td class="row-left">$!module.getRefModules()</td>
                    </tr>
                    <tr>
                        <td class="header-center">Classpath</td>
                        <td class="row-left">
                            #foreach($url in $module.getClasspathURLs())
                            $url<br>
                            #end
                        </td>
                    </tr>
                </table>
            </small>

        </td>
    </tr>
    <tr>
        <td class="header-left">
            <small>Component</small>
        </td>
        <td class="header-left">
            <small>Display Name</small>
        </td>
        <td class="header-center">
            <small>Running</small>
        </td>
        <td class="header-center">
            <small>Sessions</small>
        </td>
        <td class="header-center">
            <small>Commands</small>
        </td>
    </tr>
    <tr>
        <td class="row-left" bgcolor="#FFFFFF">
            <small><a href="/">/</a></small>
        </td>
        <td class="row-left" bgcolor="#FFFFFF">
            <small>Welcome to Tomcat</small>
        </td>
        <td class="row-center" bgcolor="#FFFFFF">
            <small>true</small>
        </td>
        <td class="row-center" bgcolor="#FFFFFF">
            <small><a href="/manager/html/sessions?path=/">0</a></small>
        </td>
        <td class="row-left" bgcolor="#FFFFFF">
            <small> &nbsp;Start&nbsp;
                &nbsp;<a href="/manager/html/stop?path=/" onClick="return(confirm('Are you sure?'))">Stop</a>&nbsp;
                &nbsp;<a href="/manager/html/reload?path=/" onClick="return(confirm('Are you sure?'))">Reload</a>&nbsp;
                &nbsp;<a href="/manager/html/undeploy?path=/" onClick="return(confirm('Are you sure?'))">Undeploy</a>&nbsp; </small>
        </td>
    </tr>
    <tr>
        <td class="row-left" bgcolor="#C3F3C3">
            <small><a href="/Tomcat">/Tomcat</a></small>
        </td>
        <td class="row-left" bgcolor="#C3F3C3">
            <small>&nbsp;</small>
        </td>
        <td class="row-center" bgcolor="#C3F3C3">
            <small>true</small>
        </td>
        <td class="row-center" bgcolor="#C3F3C3">
            <small><a href="/manager/html/sessions?path=/Tomcat">0</a></small>
        </td>
        <td class="row-left" bgcolor="#C3F3C3">
            <small> &nbsp;Start&nbsp;
                &nbsp;<a href="/manager/html/stop?path=/Tomcat" onClick="return(confirm('Are you sure?'))">Stop</a>&nbsp;
                &nbsp;<a href="/manager/html/reload?path=/Tomcat" onClick="return(confirm('Are you sure?'))">Reload</a>&nbsp;
                &nbsp;<a href="/manager/html/undeploy?path=/Tomcat" onClick="return(confirm('Are you sure?'))">Undeploy</a>&nbsp; </small>
        </td>
    </tr>
</table>
#end
<br>
<table border="1" cellspacing="0" cellpadding="3">
    <tr>
        <td colspan="2" class="title">Deploy</td>
    </tr>
    <tr>
        <td colspan="2" class="header-left">
            <small>Module file(.zip) to deploy</small>
        </td>
    </tr>
    <tr>
        <td colspan="2">
        <form method="post" enctype="multipart/form-data">
            <table cellspacing="0" cellpadding="3">
                <tr>
                    <td class="row-right">
                        <small>Select WAR file to upload</small>
                    </td>
                    <td class="row-left">
                        <input type="file" name="deployWar" size="40">
                    </td>
                </tr>
                <tr>
                    <td class="row-right">&nbsp; </td>
                    <td class="row-left">
                        <button onclick="alert('Not implemented :-)');return false;" value="Deploy">Deploy</button>
                    </td>
                </tr>
            </table>
        </form>
</table>
<br>
<hr size="1" noshade="noshade">
<center>
    <font size="-1" color="#525D76"> <em>Copyright &copy; 2002-2007, jfox.org.cn</em></font>
</center>
</body>
</html>
