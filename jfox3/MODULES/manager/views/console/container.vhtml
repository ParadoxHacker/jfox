<html>
<head>
    <link href="console/styles.css" rel="stylesheet" type="text/css">
    <title>JFox Management Console</title>
</head>
<body bgcolor="#FFFFFF">
<table cellspacing="4" width="100%" border="0">
    <tr>
        <td><a href="#"><img border="0" alt="JFox 3" align="left" src="images/jfox.jpg"></a>
            <a href="http://tomcat.apache.org/"> <img border="0" alt="The Tomcat Servlet/JSP Container" align="right" src="images/tomcat.gif"> </a>
        </td>
    </tr>
</table>
<hr size="1" noshade="noshade">
<table cellspacing="4" cellpadding="4" width="100%" border="0">
<tr>
<td width="180" class="menu">
    <br>
    <br>
    <table cellspacing="0" width="100%" border="0">
        <tr>
            <td class="title"><big>Menu</big></td>
        </tr>
        <tr>
            <td class="header-left">
                <small><a href="console.sysinfo.do">System Infomation</a></small>
            </td>
        </tr>
        <tr>
            <td class="header-left">
                <small><a href="console.jndi.do">JNDI View</a></small>
            </td>
        </tr>
        <tr>
            <td class="header-left">
                <small><a href="console.container.do">EJB Container</a></small>
            </td>
        </tr>
        <tr>
            <td class="header-left">
                <small><a href="console.jpa.do">Persistence Unit</a></small>
            </td>
        </tr>
        <tr>
            <td class="header-left">
                <small><a href="console.modules.do">Modules</a></small>
            </td>
        </tr>
    </table>
</td>
<td>
<table cellspacing="4" width="100%" border="0">
    <tr>
        <td class="page-title" bordercolor="#000000" align="left" nowrap><font size="+2">EJB Container Manager</font></td>
    </tr>
</table>
<br>
<table border="1" cellspacing="0" cellpadding="3">
    <tr>
        <td colspan="2" class="title">Transaction Manager</td>
    </tr>
    <tr>
        <td class="row-left">Transaction Timeout (seconds)</td>
        <td class="row-center">$defaultTransactionTimeout</td>
    </tr>
</table>
<br>
<table border="1" cellspacing="0" cellpadding="3">
    <tr>
        <td colspan="8" class="title">EJBs ($buckets.size())</td>
    </tr>
    <tr>
        <td class="header-left">
            <small>No.</small>
        </td>
        <td class="header-left" title="$bucket.getDescription()">
            <small>EJB name<br>
            <i>Mapped Names</i></small>
        </td>
        <td class="header-left">
            <small>Bean class<br>
            <i>Interfaces</i></small>
        </td>
        <td class="header-center">
            <small>EJB type</small><br>
            <small>WebService</small>
        </td>
        <td class="header-center">
            <small>Module</small>
        </td>
    </tr>
    #foreach($bucket in $buckets)
    <tr bgcolor="#if($velocityCount%2==0)#FFFFFF#else#C3F3C3#end">
        <td class="row-left">
            <small>$velocityCount</small>
        </td>
        <td class="row-left">
            <small><a href="console.ejbdetail.do">$bucket.getEJBName()</a>
            #foreach($mappedName in $bucket.getMappedNames())
            <br><i>$mappedName</i>
            #end
            </small>
        </td>
        <td class="row-left">
            <small>$bucket.getBeanClass().getName()
            #foreach($interfaceClass in $bucket.getBeanInterface())
            <br><i>$interfaceClass.getName()</i>
            #end
            </small>
        </td>
        <td class="row-left">
            <small>
                #if($bucket.isSession())
                    #if($bucket.isStateless())
                    Stateless
                    #else
                    Stateful
                    #end
                #else
                    MDB
                #end
            </small><br>
            <small>$bucket.isWebService()</small>
        </td>
        <td class="row-center">
            <small>$bucket.getModule().getName()</small>
        </td>
    </tr>
    #end
</table>
</td>
</tr>
</table>
<br>
<hr size="1" noshade="noshade">
<center>
    <font size="-1" color="#525D76"> <em>Copyright &copy; 1999-2005, Apache Software Foundation</em></font>
</center>
</body>
</html>
