<html>
<head>
    <title>petstore demo</title>
</head>
<body>
#parse ("IncludeTop.tmpl")

<table align="left" bgcolor="#008800" border="0" cellspacing="2" cellpadding="2">
    <tr>
        <td bgcolor="#FFFF88">
            <a href="page.index.do"><b><font color="BLACK" size="2">&lt;&lt; Main Menu</font></b></a>
        </td>
    </tr>
</table>

<p>
<center>
    <b>Please confirm the information below and then press continue...</b>
</center>
<p>
<table width="60%" align="center" border="0" cellpadding="3" cellspacing="1" bgcolor="#FFFF88">
<tr bgcolor="#FFFF88">
    <td align="center" colspan="2">
        <font size="4"><b>Order</b></font>
        <br/><font size="3"><b>$!order.getOrderDate</b></font>
    </td>
</tr>

<tr bgcolor="#FFFF88">
    <td colspan="2">
        <font color="GREEN" size="4"><b>Billing Address</b></font>
    </td>
</tr>
<tr bgcolor="#FFFF88">
    <td>
        First name:
    </td>
    <td>$!order.getBillToFirstName()
    </td>
</tr>
<tr bgcolor="#FFFF88">
    <td>
        Last name:
    </td>
    <td>$!order.getBillToLastName()
    </td>
</tr>
<tr bgcolor="#FFFF88">
    <td>
        Address 1:
    </td>
    <td>$!order.getBillAddress1()
    </td>
</tr>
<tr bgcolor="#FFFF88">
    <td>
        Address 2:
    </td>
    <td>$!order.getBillAddress2()
    </td>
</tr>
<tr bgcolor="#FFFF88">
    <td>
        City:
    </td>
    <td>$!order.getBillCity()
    </td>
</tr>
<tr bgcolor="#FFFF88">
    <td>
        State:
    </td>
    <td>$!order.getBillState()
    </td>
</tr>
<tr bgcolor="#FFFF88">
    <td>
        Zip:
    </td>
    <td>$!order.getBillZip()
    </td>
</tr>
<tr bgcolor="#FFFF88">
    <td>
        Country:
    </td>
    <td>$!order.getBillCountry()
    </td>
</tr>
<tr bgcolor="#FFFF88">
    <td colspan="2">
        <font color="GREEN" size="4"><b>Shipping Address</b></font>
    </td>
</tr>
<tr bgcolor="#FFFF88">
    <td>
        First name:
    </td>
    <td>$!order.getShipToFirstName()
    </td>
</tr>
<tr bgcolor="#FFFF88">
    <td>
        Last name:
    </td>
    <td>$!order.getShipToLastName()
    </td>
</tr>
<tr bgcolor="#FFFF88">
    <td>
        Address 1:
    </td>
    <td>$!order.getShipAddress1()
    </td>
</tr>
<tr bgcolor="#FFFF88">
    <td>
        Address 2:
    </td>
    <td>$!order.getShipAddress2()
    </td>
</tr>
<tr bgcolor="#FFFF88">
    <td>
        City:
    </td>
    <td>$!order.getShipCity()
    </td>
</tr>
<tr bgcolor="#FFFF88">
    <td>
        State:
    </td>
    <td>$!order.getShipState()
    </td>
</tr>
<tr bgcolor="#FFFF88">
    <td>
        Zip:
    </td>
    <td>$!order.getShipZip()
    </td>
</tr>
<tr bgcolor="#FFFF88">
    <td>
        Country:
    </td>
    <td>$!order.getShipCountry()
    </td>
</tr>

</table>
<p>
<center>
    <a href="order.confirm.do"><img border="0" src="images/button_continue.gif"/></a>
</center>

#include ("IncludeBottom.tmpl")
</body>
</html>